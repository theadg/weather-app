{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "indent": "error",
    "prefer-destructuring": ["error", { "object": true, "array": false }],
    "no-console": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "import/no-cycle": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off"
  }
}
